{
    "manifest_version": 2,
    "name": "Google for Black People",
    "version": "0.1",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches":[
                "https://*.google.com/search?*=*",
                "http://*.google.com/seach?*=*"
            ],
            "js": ["jquery-3.4.1.min.js","content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    }
}
